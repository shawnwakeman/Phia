{
	"semi": true,
	"singleQuote": false,
	"svelteSortOrder": "options-scripts-markup-styles",
	"svelteStrictMode": true,
	"svelteBracketNewLine": true,
	"svelteAllowShorthand": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs":true,
	"arrowParens":"always",
	"plugins": ["prettier-plugin-svelte"],
	"bracketSpacing":true,
	"proseWrap": "always",
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
  }

